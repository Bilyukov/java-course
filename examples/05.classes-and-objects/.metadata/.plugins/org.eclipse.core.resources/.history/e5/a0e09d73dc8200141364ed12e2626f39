
public class Cat {

	private String name;
	private String breed;
	private int age;
	
	public Cat(){
		this.name = "";
		this.breed = "";
		this.age = 0;
	}
	
	public Cat(String name, String breed, int age){
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		if(name != null){
			this.name = name;
		}
	}

	public String getBreed() {
		return breed;
	}

	public void setBreed(String breed) {
		if(name != null){
			this.name = name;
		}
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
	
}


public class Human extends Creature {

	private String name;
	
	public Human(String name, int age){
		setName(name);
		setAge(age);
	}
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public void greed() {
		System.out.printf(format, args);
		
	}

}

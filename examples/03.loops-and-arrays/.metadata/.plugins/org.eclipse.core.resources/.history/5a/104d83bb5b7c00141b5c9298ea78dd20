
public class ForLoop {

	public static void main(String[] args) {
		
		for (int number = 0; number < 10; number++) {
		    System.out.print(number + " ");
		}
		System.out.println();
		
		int n = 10;
		long factorial = 1;
		for (int i = 1; i <= n; i++) {
		    factorial *= i;
		}
		System.out.printf("%d! = %s\n", n, factorial);

	}

}

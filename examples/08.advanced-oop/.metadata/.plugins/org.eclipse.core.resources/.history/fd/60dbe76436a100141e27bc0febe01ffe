package polymorphism;

public class Cucumber extends Vegetable implements Growable  {

	private int size;
	
	public Cucumber(int size) {
		this.weight = weight;
		this.name = "Tomato";
		this.color = "Red";
	}
	
	@Override
	public void Grow() {
		this.weight += 10;
	}
	
	@Override
	public String toString() {
		String vegiString = super.toString();
		return vegiString + " Weight: " + this.weight;
	}

}
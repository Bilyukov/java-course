import java.util.LinkedList;
import java.util.Queue;;


public class QueueUsage {

	public static void main(String[] args) {
		
		 Queue<Integer> queue = new LinkedList<Integer>();

	        for (int i = 0; i < 5; i++) {
				queue.add(i * 5)
			}

	        while (!queue.isEmpty()) {
	            System.out.println(queue.remove());
	            Thread.sleep(1000);
	        }
	}

}

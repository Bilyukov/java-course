
public class StringEquals {

	public static void main(String[] args) {
		
		String first = "Аз Програмирам на Java";
		
		String second = "Аз Програмирам на Java";
		
		boolean refComparison = first == second;
		
		System.out.format("Сравняване на референциите с ==: %s", refComparison);
		
		boolean valueComparison = first.equals(second)
		
		

	}
}
